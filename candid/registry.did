type CanisterMetadata = record {
    name: text;
    description: text;
    url: text;
    logo_url: text;
    version: nat32;
};

service : {
    "name"               :  () -> (text) query;

    "get_info"           : (vec text) -> (vec opt CanisterMetadata) query;
    "add_canister"       : (principal, CanisterMetadata) -> ();
}